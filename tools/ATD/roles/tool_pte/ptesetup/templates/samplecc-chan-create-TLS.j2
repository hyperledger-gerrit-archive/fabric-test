    "deploy": {
        "chaincodePath": "github.com/hyperledger/fabric-test/chaincodes/samplecc/go",
        "fcn": "init",
        "args": []
    },
    "SCFile": [
               {"ServiceCredentials":"CITest/CISCFiles/config-chan{{ item.1|int }}-TLS.json"}
       ]
}
