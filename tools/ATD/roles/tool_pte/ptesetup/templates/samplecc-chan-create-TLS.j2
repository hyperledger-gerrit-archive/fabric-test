    "chaincodeVer": "v0",
    "logLevel": "ERROR",
    "nProcPerOrg": "4",
    "transType": "Channel",
    "TLS": "enabled",
    "channelOpt": {
        "name": "{{ item.0.name }}",
        "channelTX": "/var/hyperledger/fabric/artifacts/{{ item.0.name }}.tx",
